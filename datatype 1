# Variable declaration and types
age = 20  # int
name = "Gayathri"  # str
height = 5.4  # float
is_student = True  # bool

# Displaying types
print(f"Age: {age}, Type: {type(age)}")
print(f"Name: {name}, Type: {type(name)}")
print(f"Height: {height}, Type: {type(height)}")
print(f"Is Student: {is_student}, Type: {type(is_student)}")
